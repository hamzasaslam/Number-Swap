#include <iostream>
using namespace std;
int main()
{
    int a, b;
    cout << "enter the number a:\n";
    cin >> a;
    cout << "enter the number b:\n";
    cin >> b;
    cout << "\nthe number before swapping:\n"
         << "(" << a << "," << b << ")";
    a = a * b;
    b = a / b;
    a = a / b;
    cout << "\nthe number after swaping:\n"
         << "(" << a << "," << b << ")";

    return 0;
}
